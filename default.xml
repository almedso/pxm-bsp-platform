<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default revision="master" sync-j="4"/>

  <remote name="ams" fetch="git://github.com/almedso"/>
  <remote name="freescale" fetch="git://github.com/Freescale"/>

  <remote name="nodejs" fetch="https://github.com/imyller"/>
  <remote name="oe" fetch="git://git.openembedded.org"/>
  <remote name="os-systems" fetch="git://github.com/OSSystems"/>
  <remote name="yocto" fetch="git://git.yoctoproject.org"/>

  <remote name="qt5" fetch="https://github.com/meta-qt5"/>
  <remote name="linaro" fetch="git://git.linaro.org/openembedded"/>
  <remote name="openamp" fetch="https://github.com/OpenAMP"/>
  <remote name="arago" fetch="git://arago-project.org/git"/>
  <remote name="arago-projects" fetch="git://arago-project.org/git/projects"/>

  <remote name="phytec" fetch="https://stash.phytec.com/scm/pub"/>
  <remote name="ssh.phytec" fetch="ssh://git@stash.phytec.com/dev"/>


  <!-- almedso opensource projects -->

  <project name="yocto-bsp-base" path="sources/base" remote="ams">
        <copyfile dest="README.md" src="README.md"/>
        <copyfile dest="setup-environment" src="setup-environment"/>
  </project>
  <project name="yocto-ams" path="sources/ams" remote="ams"/>
  <project name="yocto-bsp-documentation" path="documentation" remote="ams"/>

  <!-- Open source collaboration projects adapted by almedso for contribution -->


  <!-- Open source collaboration projects -->

  <!-- Yocto Core --> 
  <project name="poky" revision="thud" remote="yocto" path="sources/poky"/>
  <project name="meta-openembedded" revision="thud" remote="oe" path="sources/meta-openembedded"/>

  <!-- Hardware specific -->
  <project name="meta-raspberrypi" revision="thud" remote="yocto" path="sources/meta-raspberrypi"/>
  <project name="meta-freescale" revision="thud" remote="freescale" path="sources/meta-freescale"/>
  <project name="meta-freescale-3rdparty" revision="thud" remote="freescale" path="sources/meta-freescale-3rdparty"/>
  <project name="meta-freescale-distro" revision="thud" remote="freescale" path="sources/meta-freescale-distro"/>

  <!-- Application specific -->
  <project name="meta-browser" remote="os-systems" path="sources/meta-browser" />
  <project name="meta-nodejs" remote="nodejs" path="sources/meta-nodejs" />

  <!-- other most often BSP related -->
  <project name="meta-ti" remote="yocto" revision="thud" path="sources/meta-ti"/>
  <project name="meta-linaro"  remote="linaro" revision="thud" path="sources/meta-linaro"/>
  <project name="meta-processor-sdk" remote="arago-projects" revision="master" path="sources/meta-processor-sdk"/>
  <project name="meta-arago" remote="arago" revision="master" path="sources/meta-arago"/>
  <project name="meta-openamp" remote="openamp" revision="master" path="sources/meta-openamp"/>

  <project name="meta-phytec" remote="phytec" revision="morty" path="sources/meta-phytec"/>
  <project name="meta-phytec-extra" remote="phytec" revision="morty" path="sources/meta-phytec-extra"/>


</manifest>
